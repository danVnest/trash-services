				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run AppleScript.action</string>
				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {theVolumes, parameters}	set keysPressed to do shell script "/usr/local/bin/modKeys control"	set controlPressed to do shell script "echo $((" &amp; keysPressed &amp; "&amp;4096?1:0))"	set optionPressed to do shell script "echo $((" &amp; keysPressed &amp; "&amp;2048?1:0))"		if controlPressed is equal to "1" then		set userID to ""		set permissions to "sudo"	else		set userID to user ID of (system info) &amp; "/"		set permissions to ""	end if		if optionPressed is equal to "1" then		set rm to "srm"	else		set rm to "rm"	end if		repeat with theVolume in theVolumes		tell application "Finder" to set volumeName to name of theVolume		set trashPath to quoted form of POSIX path of theVolume &amp; ".Trashes/" &amp; userID		try			if (count (paragraphs of (do shell script permissions &amp; "ls -l " &amp; trashPath))) &gt; 0 then				try					do shell script permissions &amp; "rm -r " &amp; trashPath &amp; "*"				on error					display dialog "Error: " &amp; volumeName &amp; "'s trash could not be emptied." buttons {"OK"} default button "OK"				end try			else				display dialog volumeName &amp; "'s trash is already empty." buttons {"OK"} default button "OK"			end if		on error			display dialog volumeName &amp; " has no trash folder." buttons {"OK"} default button "OK"		end try	end repeatend run</string>
